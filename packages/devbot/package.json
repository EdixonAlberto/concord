{
  "name": "devbot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -e ts -w ./src --exec \"yarn build && yarn start\"",
    "alias-install": "link-module-alias",
    "alias-clean": "link-module-alias clean || true",
    "build": "tsc",
    "start": "yarn alias-install && node dist"
  },
  "_moduleAliases": {
    "~HELP": "dist/helpers"
  },
  "dependencies": {
    "@edixon/concord": "1.0.0",
    "link-module-alias": "1.2.0",
    "scrape-it": "5.2.4"
  },
  "devDependencies": {
    "dotenv": "8.2.0",
    "nodemon": "2.0.4",
    "typescript": "4.0.3"
  }
}